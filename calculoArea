import java.util.Locale;
import java.util.Scanner;

public class Main {
	
	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		Locale.setDefault(Locale.US);
		double raio, area, pi;
    
		raio = sc.nextDouble();

		pi = 3.14159;
		area = pi * raio * raio;
		
	
		System.out.printf("Area = %.4f%n", area);
		
		
		
	
		sc.close();
	}
}
