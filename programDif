import java.util.Scanner;

public class Main {
	
	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		int a = 0, b = 0, c = 0, d = 0;
		int dif;
    
		a = sc.nextInt();
		b = sc.nextInt();
		c = sc.nextInt();
		d = sc.nextInt();
		

		dif = a * b - c * d;
		
	
		System.out.println("Diferen√ßa = " + dif);
		
		
		sc.close();
	}
}
